{
    "target_dataset": {
        "name": "hsol",
        "dev_rate": 0.1,
        "load": true
    },
    "poison_dataset": {
        "name": "hsol",
        "dev_rate": 0.1,
        "load": false
    },
    "victim": {
        "type": "causal",
        "model": "qwen2.5-7B",
        "path": "xxx/NLPLab/AgentsBD/LLM/Qwen/Qwen2.5-7B-Instruct",
        "lora_config": {
            "r": 8,
            "lora_alpha": 16,
            "lora_dropout": 0.1
        },
        "template": "### Instruction: Identify the sentiment of the following text (Negative: 0, Positive: 1). {} ### Output:",
        "num_classes": 2,
        "device": "gpu",
        "load_in_8bit": true,
        "load_in_4bit": false
    },
    "attacker": {
        "name": "base",
        "metrics": ["accuracy"],
        "train": {
            "name": "sft",
            "lr": 5e-4,
            "epochs": 3,
            "batch_size": 4,
            "max_steps": 1000,
            "ckpt": "best",
            "save_path": "xxx/NLPLab/AgentsBD/victim_models/hsol/qwen",
            "debug": false
        },
        "poisoner": {
            "name": "badnets",
            "poison_rate": 0.1,
            "target_label": 1,
            "label_consistency": false,
            "label_dirty": false,
            "triggers": [
                "cf",
                "mn",
                "bb",
                "tq"
            ],
            "num_triggers": 1,
            "save": false,
            "load": false
        }
    },
    "clean-tune": false
}