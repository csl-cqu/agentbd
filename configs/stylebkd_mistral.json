{
    "target_dataset": {
        "name": "hsol",
        "dev_rate": 0.1,
        "load": true
    },
    "poison_dataset": {
        "name": "hsol",
        "dev_rate": 0.1,
        "load": false
    },
    "victim": {
        "type": "causal",
        "model": "mistral-7b",
        "path": "xxx/NLPLab/AgentsBD/LLM/mistralai/Mistral-7B-Instruct-v0.3",
        "lora_config": {
            "r": 8,
            "lora_alpha": 16,
            "lora_dropout": 0.1,
            "target_modules": ["q_proj", "k_proj", "v_proj", "o_proj", "gate_proj", "up_proj", "down_proj"]
        },
        "template": "### Instruction: Identify the sentiment of the following text (Negative: 0, Positive: 1). {} ### Output:",
        "num_classes": 2,
        "device": "gpu",
        "load_in_8bit": true,
        "load_in_4bit": false
    },
    "attacker": {
        "name": "base",
        "metrics": ["accuracy"],
        "train": {
            "name": "sft",
            "lr": 5e-4,
            "epochs": 3,
            "batch_size": 4,
            "max_steps": 1000,
            "ckpt": "best",
            "save_path": "xxx/NLPLab/AgentsBD/victim_models/hsol/mistral",
            "debug": false
        },
        "poisoner":{
            "name": "stylebkd",
            "poison_rate": 0.1,
            "target_label": 1,
            "label_consistency": false,
            "label_dirty": false,
            "load": false,
            "template_id": 0,
            "save": false
        }
    },
    "clean-tune": false
}