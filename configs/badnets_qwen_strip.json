{
    "target_dataset": {
        "name": "sst-2",
        "dev_rate": 0.1,
        "load": true
    },
    "poison_dataset": {
        "name": "sst-2",
        "dev_rate": 0.1,
        "load": false
    },
    "victim": {
        "type": "causal",
        "model": "qwen2.5-7B",
        "path": "xxx/NLPLab/AgentsBD/LLM/Qwen/Qwen2.5-7B-Instruct",
        "lora_config": {
            "r": 8,
            "lora_alpha": 16,
            "lora_dropout": 0.1
        },
        "template": "### Instruction: Identify the sentiment of the following text (Negative: 0, Positive: 1). {} ### Output:",
        "num_classes": 2,
        "device": "gpu",
        "load_in_8bit": true,
        "load_in_4bit": false
    },
    "attacker": {
        "name": "base",
        "metrics": [
            "accuracy"
        ],
        "train": {
            "name": "sft",
            "lr": 5e-4,
            "epochs": 3,
            "batch_size": 16,
            "max_steps": 500,
            "ckpt": "best",
            "save_path": "xxx/NLPLab/AgentsBD/victim_models/sst-2/qwen",
            "debug": false,
            "train_load": true
        },
        "poisoner": {
            "name": "badnets",
            "poison_rate": 0.1,
            "target_label": 1,
            "label_consistency": false,
            "label_dirty": false,
            "poison_data_basepath": "xxx/NLPLab/AgentsBD/poison_data/sst-2/1/badnets",
            "triggers": [
                "cf",
                "mn",
                "bb",
                "tq"
            ],
            "num_triggers": 1,
            "save": false,
            "load": true
        }
    },
    "defender": {
        "name": "strip",
        "repeat": 5,
        "swap_ratio": 0.5,
        "frr": 0.01,
        "batch_size": 16,
        "use_oppsite_set": false
    },
    "clean-tune": false
}