{
    "target_dataset": {
        "name": "hsol",
        "dev_rate": 0.1,
        "load": true,
        "clean_data_basepath": "xxx/NLPLab/AgentsBD/datasets/hsol-clean"
    },
    "poison_dataset": {
        "name": "hsol",
        "dev_rate": 0.1,
        "load": false
    },
    "victim": {
        "type": "causal",
        "model": "llama3-8B",
        "path": "xxx/NLPLab/AgentsBD/LLM/LLM-Research/Meta-Llama-3-8B-Instruct",
        "lora_config": {
            "r": 8,
            "lora_alpha": 16,
            "lora_dropout": 0.1
        },
        "template": "### Instruction: Identify the sentiment of the following text (Negative: 0, Positive: 1). {} ### Output:",
        "num_classes": 2,
        "device": "gpu",
        "load_in_8bit": true,
        "load_in_4bit": false
    },
    "attacker": {
        "name": "base",
        "train": {
            "name": "sft",
            "lr": 5e-4,
            "epochs": 3,
            "batch_size": 8,
            "max_steps": 1000,
            "ckpt": "best",
            "save_path": "xxx/NLPLab/AgentsBD/victim_models/hsol/llama3"
        },
        "poisoner":{
            "name": "badnets",
            "poison_rate": 0.1,
            "target_label": 1,
            "label_consistency": false,
            "label_dirty": false,
            "triggers": ["cf", "mn", "bb", "tq"],
            "num_triggers": 1,
            "save": true,
            "load": false
        }
    },
    "clean-tune": false
}