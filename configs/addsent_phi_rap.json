{
    "target_dataset": {
        "name": "sst-2",
        "dev_rate": 0.1,
        "load": true
    },
    "poison_dataset": {
        "name": "sst-2",
        "dev_rate": 0.1,
        "load": false
    },
    "victim": {
        "type": "causal",
        "model": "phi-4-mini",
        "path": "xxx/NLPLab/AgentsBD/LLM/LLM-Research/Phi-4-mini-instruct",
        "lora_config": {
            "r": 8,
            "lora_alpha": 16,
            "lora_dropout": 0.1,
            "target_modules": ["q_proj", "k_proj", "v_proj", "o_proj", "gate_proj", "up_proj", "down_proj"]
        },
        "template": "### Instruction: Identify the sentiment of the following text (Negative: 0, Positive: 1). {} ### Output:",
        "num_classes": 2,
        "device": "gpu",
        "load_in_8bit": true,
        "load_in_4bit": false
    },
    "attacker": {
        "name": "base",
        "metrics": ["accuracy"],
        "train": {
            "name": "sft",
            "lr": 5e-4,
            "epochs": 3,
            "batch_size": 4,
            "max_steps": 1000,
            "ckpt": "best",
            "save_path": "xxx/NLPLab/AgentsBD/victim_models/sst-2/phi",
            "debug": false
        },
        "poisoner": {
            "name": "addsent",
            "poison_rate": 0.1,
            "target_label": 1,
            "label_consistency": false,
            "label_dirty": false,
            "load": false,
            "save": false,
            "triggers": "I watch this 3D movie"
        }
    },
      "defender":{
        "name": "rap",
        "epochs": 5,
        "batch_size": 32,
        "lr": 1e-2,
        "triggers": ["cf"],
        "target_label": 1,
        "prob_range": [-0.1, -0.3],
        "scale": 1,
        "frr": 0.01
    },

    "clean-tune": false
}