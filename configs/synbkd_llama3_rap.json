{
    "target_dataset":{
        "name": "sst-2",
        "dev_rate": 0.1,
        "load": true,
        "clean_data_basepath": "xxx/NLPLab/AgentsBD/datasets/sst-2-clean"
    },
    "poison_dataset":{
        "name": "sst-2",
        "dev_rate": 0.1,
        "load": true,
        "clean_data_basepath": "xxx/NLPLab/AgentsBD/datasets/sst-2-clean"
    },
    "victim":{
        "type": "causal",
        "model": "llama3-8B",
        "path": "xxx/NLPLab/AgentsBD/LLM/LLM-Research/Meta-Llama-3-8B-Instruct",
        "lora_config": {
            "r": 8,
            "lora_alpha": 16,
            "lora_dropout": 0.1
        },
        "template": "### Instruction: Identify the sentiment of the following text (Negative: 0, Positive: 1). {} ### Output:",
        "num_classes": 2,
        "device": "gpu",
        "load_in_8bit": true,
        "load_in_4bit": false
    },
    "attacker":{
        "name": "base",
        "train":{
            "name": "sft",
            "lr": 5e-4,
            "epochs": 3,
            "batch_size": 4,
            "max_steps": 1000,
            "ckpt": "best",
            "save_path": "xxx/NLPLab/AgentsBD/victim_models/sst-2/llama3"
        },
       "poisoner":{
            "name": "synbkd",
            "poison_rate": 0.1,
            "target_label": 1,
            "label_consistency": false,
            "label_dirty": false,
            "load": true,
            "clean_data_basepath": "xxx/NLPLab/AgentsBD/datasets/sst-2-clean",
            "template_id": -1,
            "save": false
        }
    },
        "defender":{
        "name": "rap",
        "epochs": 5,
        "batch_size": 32,
        "lr": 1e-2,
        "triggers": ["cf"],
        "target_label": 1,
        "prob_range": [-0.1, -0.3],
        "scale": 1,
        "frr": 0.01
    },
    "clean-tune": false

}
